#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string s;
    while(getline(cin,s)&&s!="DONE"){
        string s1,s2;
     for (int i=0;i<s.length();i++){
        if(isdigit(s[i]))            // we can also use isalpha instead of isdigit
            s1+=tolower(s[i]);

        }

    s2=s1;
    reverse(s2.begin(),s2.end());
    if (s1==s2)
        cout<<"You won't be eaten!"<<"\n";
    else
               cout<<"Uh oh.."<<"\n";
    }

      return 0;
    }

